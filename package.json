{
  "name": "redux-boilerplate",
  "description": "Boilerplate for react-redux quick start",
  "version": "0.1.0",
  "scripts": {
    "start": "DEBUG=true node server.js",
    "build": "webpack -p --config webpack.production.js",
    "build-dev": "webpack -p",
    "lint": "./node_modules/.bin/standard",
    "test": "./node_modules/.bin/jest"
  },
  "dependencies": {
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-devtools": "^3.0.1",
    "redux-router": "^1.0.0-beta7",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.0.0",
    "babel-jest": "^6.0.1",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.22.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "image-webpack-loader": "^1.6.2",
    "jest-cli": "^0.8.2",
    "jsdom": "^7.2.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "react-addons-test-utils": "^0.14.5",
    "react-hot-loader": "^1.3.0",
    "standard": "^5.4.1",
    "style-loader": "^0.13.0",
    "traceur": "0.0.95",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.12.1"
  },
  "standard": {
    "globals": [
      "jest",
      "describe",
      "it",
      "expect"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-preprocessor.js",
    "setupEnvScriptFile": "<rootDir>/node_modules/traceur/bin/traceur-runtime.js",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
}
